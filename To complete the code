#include <assert.h>
#include <ctype.h>
#include <limits.h>
#include <math.h>
#include <stdbool.h>
#include <stddef.h>
#include <stdint.h>
#include <stdio.h>
#include <stdlib.h>
#include <string.h>



int main()
{
  
    int num1=10;

    float num2=10.6;

    char ch='a';

    float num3=10.6;

     printf("%d\n",num1);

     printf("%c\n",ch);

     printf("%lf\n",num2);

     printf("%lf",num3);

    return 0;
}
