#include <assert.h>
#include <ctype.h>
#include <limits.h>
#include <math.h>
#include <stdbool.h>
#include <stddef.h>
#include <stdint.h>
#include <stdio.h>
#include <stdlib.h>
#include <string.h>



int main()
{
   

    int a=10,b=2;

    
      printf("Addition=%d\n",(a+b));
     

   
      printf("Subtraction=%d\n",(a-b));
     

    
      printf("Multiplication=%d\n",(a*b));
     

    
      printf("Division=%d",(a/b));
     

    return 0;
}
